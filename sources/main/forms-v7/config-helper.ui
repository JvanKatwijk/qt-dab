       </property>
       <property name="text">
        <string>load table</string>
       </property>
      </widget>
     </item>
     <item row="1" column="4">
      <widget class="smallPushButton" name="dumpButton">
       <property name="sizePolicy">
        <sizepolicy hsizetype="Preferred" vsizetype="Fixed">
         <horstretch>0</horstretch>
         <verstretch>0</verstretch>
        </sizepolicy>
       </property>
       <property name="font">
